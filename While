#include <stdio.h>

int main() {
    int n, flag=0, i=2;
    printf("enter an integer number \n");
    scanf("%d",&n);
    while(i<=n/2)
    {
        if(n% i==0)
        {
            flag=1;
            break;
        }
        i++;
    }
    if (flag==0)
    printf("it is a prime number \n");
    else
    printf("not a prime number \n");
        
        
    
    

    return 0;
}
