int main() {
    int *arr=(int*)malloc(10*sizeof(int));
    for(int i=0;i<10;i++){
        printf("enter element %d",i+1);
        scanf("%d",&arr[i]);
        
        
    }
printf("arry element:\n");
for(int i=0;i<10;i++){
    printf("%d",arr[i]);
}
free (arr);
for(int i=0;i<10;i++)
    return 0;
}
