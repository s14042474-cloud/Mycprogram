#include <stdio.h>

int sicalculater() {
    int p,t,r;
    printf("enter your principle amount\n");
    scanf("%d",&p);
    printf("enter the number of years\n");
    scanf("%d",&t);
    printf("enter a rate of interest \n");
    scanf("%d",&r);
    return (p*t*r)/100;
}
int main(){
    sicalculater();



    return 0;
}
